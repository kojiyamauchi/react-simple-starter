    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "cache-loader": "^4.1.0",
    "chokidar": "^3.5.1",
    "chokidar-cli": "^2.1.0",
    "copy-webpack-plugin": "^7.0.0",
    "cpx": "^1.5.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "exports-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^5.0.0-alpha.1",
    "husky": "^4.3.8",
    "imagemin": "^7.0.1",
    "imagemin-webp": "^6.0.0",
    "imports-loader": "^1.2.0",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "json-server": "^0.16.3",
    "lint-staged": "^10.5.3",
    "node-fetch": "^2.6.1",
    "npm-check-updates": "^11.0.2",
    "npm-run-all": "^4.1.5",
    "opener": "^1.5.2",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.14.2",
    "react-hot-loader": "^4.13.0",
    "react-query": "^3.6.0",
    "react-redux": "^7.2.2",
    "react-refresh": "^0.9.0",
    "react-router-dom": "^5.2.0",
    "react-svg-loader": "^3.0.3",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.0",
    "styled-components": "^5.2.1",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "svg-inline-loader": "^0.8.2",
    "swr": "^0.4.0",
    "terser-webpack-plugin": "^5.1.1",
    "thread-loader": "^3.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "wait-on": "^5.2.1",
    "webpack": "^5.16.0",
    "webpack-build-notifier": "^2.1.1",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "modules": false,
          "corejs": 3
        }
      ],
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      [
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "displayName": true,
          "transpileTemplateLiterals": true
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "resource/**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix --max-warnings 0",
      "stylelint"
    ],
    "base.config.ts": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ],
    "webpack/*.js": "prettier --write",
    "webp.js": "prettier --write"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^@/(.+)": "<rootDir>/resource/apps/$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/resource/tests/**/*.test.+(ts|tsx|js|jsx)"
    ]
  },
  "config": {
    "path": {
      "prefix": "/react-simple-starter/"
    }
  },
  "scripts": {
    "setup": "rm -rf .git",
    "webp": "node webp.js",
    "watch:webp": "chokidar 'resource/apps/materials/toWebps/*.{jpg,png}' -c 'yarn webp'",
    "start:dev": "webpack serve --inline --hot --config webpack/webpack.dev.babel.js --mode development",
    "dev": "npm-run-all -p watch:webp start:dev",
    "start:build": "webpack --config webpack/webpack.pro.babel.js --mode production",
    "build": "npm-run-all -p webp start:build",
    "clean": "rimraf delivery/* && touch delivery/.gitkeep",
    "jest": "jest --passWithNoTests",
    "check-pkg": "ncu",
    "update-pkg": "ncu '/^(?!(@add-module-name|@add-module-name)).*$/' -u",
    "re-install": "rm yarn.lock && rm -rf node_modules && yarn install"
  }
}
