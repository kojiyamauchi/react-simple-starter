  "config": {
    "path": {
      "prefix": "/react-simple-starter/"
    }
  },
  "scripts": {
    "setup": "rm -rf .git",
    "prepare": "husky install",
    "webp": "node webp.js",
    "watch:webp": "chokidar 'resource/materials/toWebps/*.{jpg,png}' -c 'yarn webp'",
    "start:dev": "webpack serve --inline --hot --config webpack/webpack.dev.babel.js --mode development",
    "start:storybook": "BROWSER='google chrome canary' start-storybook -p 6006",
    "dev": "npm-run-all -p webp watch:webp start:dev start:storybook",
    "start:build": "webpack --config webpack/webpack.pro.babel.js --mode production",
    "build-storybook": "build-storybook -o delivery/storybook",
    "build": "npm-run-all -p webp start:build build-storybook",
    "clean": "rimraf delivery/* && touch delivery/.gitkeep",
    "jest": "jest --passWithNoTests",
    "add:fc": "hygen add component",
    "add:page": "hygen add page",
    "add:hooks": "hygen add hooks",
    "add:store": "hygen add store",
    "check-pkg": "ncu",
    "update-pkg": "ncu '/^(?!(imagemin$|jest|ts-jest)).*$/' -u",
    "re-install": "rm yarn.lock && rm -rf node_modules && yarn install"
  }
